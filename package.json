{
  "name": "webviewer-react",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.ts",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "node ./esbuild.js && npm run ts-types",
    "deploydemo": "npm run beforestart && esbuild --bundle examples/Context/index.js --loader:.js=jsx --outfile=www/main.js && firebase deploy --only hosting:pdftron-ychen02",
    "removewww": "rm -rf www",
    "copypublic": "mkdir www && cp -r examples/assets/. www/",
    "copybuild": "cp -r lib www/lib",
    "beforestart": "npm run removewww && npm run build && npm run copypublic && npm run copybuild && node copy-webviewer-file.js",
    "start": "npm run beforestart && esbuild --bundle examples/Context/index.js --loader:.js=jsx --outfile=www/main.js --servedir=www",
    "ts-types": " tsc -d --emitDeclarationOnly --outDir lib/context",
    "installpeer": "npm i react react-dom @types/react @types/react-dom @pdftron/webviewer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PDFTron/webviewer-react.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PDFTron/webviewer-react/issues"
  },
  "homepage": "https://github.com/PDFTron/webviewer-react#readme",
  "devDependencies": {
    "esbuild": "^0.14.1",
    "fs-extra": "^10.0.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@pdftron/webviewer": "^8.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
